<!DOCTYPE html>
<html dir="ltr" lang="tr">

<head>
    <meta charset="utf-8">
    <!-- Tell the browser to be responsive to screen width -->
    {% block head %} {% include('home/head.html.twig') %} {% endblock %}
    <title>{% block title %}Bueno -Food Blog{% endblock %}</title>
    <meta name="description" content="{% block description %}En iyi yemek blogu{% endblock %}">
    <meta name="keywords" content="{% block keywords %}yemek ,blog{% endblock %}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

</head>

<body>

<div class="preloader d-flex align-items-center justify-content-center">
    <div class="preloader-content">
        <h3>Cooking in progress..</h3>
        <div id="cooking">
            <div class="bubble"></div>
            <div class="bubble"></div>
            <div class="bubble"></div>
            <div class="bubble"></div>
            <div class="bubble"></div>
            <div id="area">
                <div id="sides">
                    <div id="pan"></div>
                    <div id="handle"></div>
                </div>
                <div id="pancake">
                    <div id="pastry"></div>
                </div>
            </div>
        </div>
    </div>
</div>

    {% block header %}{{ include ('home/header.html.twig') }}{% endblock %}
    {% block slider %}{{ include ('home/slider.html.twig') }}{% endblock %}
    {% block treadingPost %}{{ include ('home/treadingPost.html.twig') }}{% endblock %}
    {% block categoryArea %}{{ include ('home/categoryArea.html.twig') }}{% endblock %}
    {% block body %} {% endblock %}
    {% block instagramArea %}{{ include ('home/instagramArea.html.twig') }}{% endblock %}
    {% block footer %}{{ include ('home/footer.html.twig') }}{% endblock %}
    {% block javascripts %}{{ include ('home/javascripts.html.twig') }}{% endblock %}

</body>

</html>